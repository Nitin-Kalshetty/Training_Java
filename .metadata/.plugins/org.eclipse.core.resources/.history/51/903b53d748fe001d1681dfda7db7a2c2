package com.rocker.ConstructorInjection;

public class ConstructorByIndex {
	
	private Integer a;
	private Integer b;
	private boolean c;
	private String d;
	public ConstructorByIndex(Integer a, Integer b, boolean c, String d) {
		super();
		this.a = a;
		this.b = b;
		this.c = c;
		this.d = d;
	}
	public ConstructorByIndex() {
		super();
	}
	public Integer getA() {
		return a;
	}
	public void setA(Integer a) {
		this.a = a;
	}
	public Integer getB() {
		return b;
	}
	public void setB(Integer b) {
		this.b = b;
	}
	public boolean getC() {
		return c;
	}
	public void setC(boolean c) {
		this.c = c;
	}
	public String getD() {
		return d;
	}
	public void setD(String d) {
		this.d = d;
	}
	@Override
	public String toString() {
		return "ConstructorByIndex [a=" + a + ", b=" + b + ", c=" + c + ", d=" + d + "]";
	}
	

}
